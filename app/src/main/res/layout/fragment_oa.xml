<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:orientation="vertical">

    <com.worldpeak.chnsmilead.view.CommonTitleBar
        android:id="@+id/titleBar"
        android:layout_width="match_parent"
        android:layout_height="44dp"
        android:layout_marginTop="30dp"
        app:hideLeftIcon="true"
        app:title="移动办公"
        app:titleColor="@color/black"
        app:titleSize="16sp"
        app:titleStyle="1" />

    <include layout="@layout/layout_upgrade_text"
        android:id="@+id/clUpgrade"/>

    <include layout="@layout/layout_document_approve"
        android:id="@+id/clDocument"/>

    <include layout="@layout/layout_area_stats"
        android:id="@+id/clArea"/>


</LinearLayout>